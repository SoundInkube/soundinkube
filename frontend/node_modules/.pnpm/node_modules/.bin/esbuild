#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules/esbuild/bin/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules/esbuild/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules/esbuild/bin/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules/esbuild/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/esbuild@0.25.6/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
