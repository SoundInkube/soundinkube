#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
