#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/lib/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/tailwindcss@3.4.17/node_modules:/data/chats/49efa2fd6e454c5f8329a03d1be7c384/workspace/soundinkube2-repo/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
